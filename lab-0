#include<stdio.h>

int main()
{
	int n=0,i,max,arr[10];
	printf("enter the number of integer : ");
	scanf("%d",&n);
	printf("enter the value ");
	for(i=0;i<n;i++)
		scanf("%d",&arr[i]);
	max=arr[0];
		for(i=0;i<n;i++){
			if(max<arr[i])
				max=arr[i];
		}
	printf("max element is %d\n",max);
	return 0;
	}
